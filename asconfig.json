{
	"targets": {
		"raw": {
			"bindings": "raw",
			"outFile": "build/debug-raw.wasm",
			"textFile": "build/debug-raw.wat",
			"sourceMap": true,
			"debug": true
		},
		"debug": {
			"bindings": "esm",
			"outFile": "build/debug.wasm",
			"textFile": "build/debug.wat",
			"sourceMap": true,
			"debug": true
		},
		"release": {
			"bindings": "esm",
			"outFile": "build/release.wasm",
			"textFile": "build/release.wat",
			"sourceMap": true,
			"optimizeLevel": 3,
			"shrinkLevel": 0,
			"converge": true,
			"noAssert": true
		},
		"release-mini": {
			"bindings": "esm",
			"outFile": "build/release-mini.wasm",
			"textFile": "build/release-mini.wat",
			"sourceMap": true,
			"optimizeLevel": 3,
			"shrinkLevel": 0,
			"converge": true,
			"noAssert": true,
			"runtime": "minimal",
			"exportRuntime": true
		},
		"release-stub": {
			"bindings": "esm",
			"outFile": "build/release-stub.wasm",
			"textFile": "build/release-stub.wat",
			"sourceMap": true,
			"optimizeLevel": 3,
			"shrinkLevel": 0,
			"converge": true,
			"noAssert": true,
			"runtime": "stub",
			"exportRuntime": true
		},
		"test": {
			"bindings": "esm",
			"outFile": "build/test.wasm",
			"textFile": "build/test.wat",
			"sourceMap": true,
			"debug": true
		}
	},
	"options": {}
}
